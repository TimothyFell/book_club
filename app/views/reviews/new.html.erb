<%= form_for [@book, @review] do |f| %>
  <%= f.label :review_title %>
  <%= f.text_field :review_title %>
  <%= f.label :rating %>
  <%= f.number_field :rating %>
  <%= f.label :user %>
  <%= f.text_field :user %>
  <%= f.label :review_text %>
  <%= f.text_field :review_text %>
  <%= f.submit %>
<%end%>
